<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe />
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_marginEnd="71dp"
            motion:layout_constraintVertical_bias="0.507"
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toStartOf="@+id/textView3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/textView"
            motion:layout_constraintHorizontal_bias="0.0"
            android:layout_width="wrap_content"
            android:layout_marginStart="16dp"
            android:id="@+id/textView4" />
        <Constraint
            android:layout_marginEnd="16dp"
            android:layout_height="86dp"
            android:layout_marginRight="16dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/textView2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="204dp"
            motion:layout_constraintVertical_bias="0.102"
            android:id="@+id/textView3" />
        <Constraint
            motion:layout_constraintVertical_bias="0.305"
            android:layout_height="151dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="44dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="301dp"
            motion:layout_constraintTop_toBottomOf="@+id/textView2"
            android:id="@+id/okButton"
            motion:layout_constraintHorizontal_bias="0.49" />
        <Constraint
            android:layout_marginEnd="93dp"
            android:layout_height="46dp"
            android:layout_marginTop="116dp"
            motion:layout_constraintHorizontal_bias="1.0"
            android:layout_width="105dp"
            motion:layout_constraintTop_toBottomOf="@+id/textView4"
            motion:layout_constraintEnd_toStartOf="@+id/textView2"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"
            android:id="@+id/textView" />
        <Constraint
            android:layout_marginEnd="56dp"
            android:layout_height="67dp"
            android:layout_marginTop="52dp"
            android:layout_marginRight="56dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="137dp"
            motion:layout_constraintTop_toBottomOf="@+id/textView3"
            android:id="@+id/textView2" />
    </ConstraintSet>
</MotionScene>